<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>MongoChains.Library</AssemblyName>
    <Version>0.2.1-alpha</Version>
    <AssemblyVersion>0.2.1.0</AssemblyVersion>
    <FileVersion>0.2.1.0</FileVersion>
    <Authors>James Freiwirth</Authors>
    <Company>Perfect Shuffle Ltd</Company>
    <Copyright>ASOS</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/Apache-2.0</PackageLicenseUrl>
    <PackageTags>mongodb</PackageTags>
    <Description>MongoChains is a migrations tool for MongoDB databases.</Description>
    <PackageId>MongoChains</PackageId>
    <Product>MongoChains</Product>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ChessieExtensions.fs" />
    <Compile Include="Migrations.fs" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>